ter((meal) => meal.type === filter);\r\n  console.log(meals);\r\n\r\n  const Meals = FilteredMeals.map((meal) => (\r\n    <MealItem\r\n      key={meal.id}\r\n      id={meal.id}\r\n      name={meal.name}\r\n      description={meal.description}\r\n      price={meal.price}\r\n      image={meal.image}\r\n    />\r\n  ));\r\n\r\n  return (\r\n    <section>\r\n      <Card>\r\n        <div className={classes.buttonGroup}>\r\n          <button onClick={() => FilterMealsHandler(\"all\")}>همه</button>\r\n          <button onClick={() => FilterMealsHandler(\"pizza\")}>پیتزا</button>\r\n          <button onClick={() => FilterMealsHandler(\"sandwich\")}>ساندویچ</button>\r\n          <button style={{ marginRight: '10px' }} onClick={() => FilterMealsHandler(\"salad\")}>\r\n            سالاد\r\n          </button>\r\n          <ComboBox meals={FilteredMeals.map(meal => meal.name)} />\r\n        </div>\r\n        {isLoading && !httpError ? (\r\n          <h3 style={{ display: \"flex\", justifyContent: \"center\" }}>\r\n            {\" \"}\r\n            <Spinner />{\" \"}\r\n          </h3>\r\n        ) : (\r\n          <ul style={{ with: \"90%\" }}>{Meals} </ul>\r\n        )}\r\n        {httpError ? (\r\n          <div>\r\n            <div className={classes.httpError}> {httpError} </div>\r\n            <div className={classes.httpError}>\r\n              {\" \"}\r\n              <h3 style={{ fontSize: \"14px\" }}>\r\n                {\" \"}\r\n                please connect to proxy and reload the page.{\" \"}\r\n              </h3>{\" \"}\r\n            </div>\r\n          </div>\r\n        ) : null}\r\n      </Card>\r\n    </section>\r\n  );\r\n}\r\nexport default AvailableMeals;\r\n","F:\\React JS\\React-Max\\projects\\11\\11-Building a Food Order App\\starting-practice-project\\src\\Components\\Meals\\MealsSummary.js",[],"F:\\React JS\\React-Max\\projects\\11\\11-Building a Food Order App\\starting-practice-project\\src\\Components\\Layout\\CustomButton.js",[],"F:\\React JS\\Re